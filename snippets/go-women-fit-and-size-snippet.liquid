            <div class="sg-fit-guide-wrapper">
              <div class="sg-divider"></div>
              <h3 class="sg-size-guide-title">{{ first_name }}</h3>
              <h4 class="sg-size-guide-subtitle">This jacket has a {{ fit_product.metafields.product_information.product_fit }} fit</h4>
              <div class="sg-fit-layout-grid">
                <div class="sg-fit-embed w-embed w-script">
                  <style>
                        .sg-fit-guide-grid-block{
                                grid-auto-flow: column;
                    						overflow-y: auto;
                                overscroll-behavior-x: contain;
                                scroll-snap-type: x mandatory;
                        }

                        .sg-fit-guide-grid-block .sg-fit-block {
                          			scroll-snap-align: center;
                                object-fit: contain;
                      }
                    }
                  </style>

                  <script>


                    window.addEventListener('DOMContentLoaded', (event) => {

                      const fitGuide = document.getElementsByClassName("sg-size-fit-guide-tab")[0];
                      const mObserver = new MutationObserver(checkActive);
                      const sObserver = new ResizeObserver(scrollTo);
                      mObserver.observe(fitGuide, {childList: true, attributes:true, subtree: true, attributeFilter: ["aria-selected"]});
                      sObserver.observe(document.getElementsByClassName('sg-fit-block')[0]);
                      scrollTo()
                    });

                    function checkActive(mutationList){
                      mutationList.forEach((mutation) => {
                        if(mutation.target.getAttribute("aria-selected") == "true"){
                          mutation.target.classList.add("sg-current")
                        }else if ( mutation.target.getAttribute("aria-selected") == "false"){
                          mutation.target.classList.remove("sg-current")
                        } 
                      })
                    }

                    function scrollTo(){
                      const element = document.getElementsByClassName('sg-fit-block')[{{ index }}];

                      console.log(element.offsetLeft);
                      document.getElementsByClassName('sg-fit-guide-grid-block')[0].scrollLeft = element.offsetLeft - element.offsetWidth/3;
                    }



                    
                   

                  </script>
